    <link name="lidar_link">
      <pose>.05 0 .09 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.17</mass>
        <inertia>
          <ixx>0.0001926666667</ixx> <!-- (.17/12)*(.1^2+.06^2) -->
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001224141667</iyy> <!-- (.17/12)*(.071^2+.06^2) -->
          <iyz>0</iyz>
          <izz>0.0002130808333</izz> <!-- (.17/12)*(.071^2+.1^2) -->
        </inertia>
      </inertial>
      <visual name="visual">
        <pose>0 0 -.028 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>meshes/STL27L.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <sensor name="lidar" type="gpu_lidar">
        <lidar>
          <scan>
            <horizontal>
              <samples>400</samples>
              <resolution>1</resolution>
              <min_angle>-1.5707</min_angle>
              <max_angle>1.5707</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>25</max>
            <resolution>0.01</resolution>
          </range>
        </lidar>
        <gz_frame_id>lidar_link</gz_frame_id>
        <alwaysOn>1</alwaysOn>
        <visualize>true</visualize>
        <update_rate>10</update_rate>
      </sensor>
    </link>         


    <joint name="lidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>lidar_link</child>
    </joint>
   
   
    <link name="camera_link">
      <!-- Position the camera_link as needed without rotation -->
      <pose>0.05 0 0.15 0 0 0</pose>

      <visual name="camera_visual">
        <geometry>
          <box>
            <size>0.01 0.01 0.01</size> 
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.4 0.4</ambient>
          <diffuse>0.4 0.4 0.4</diffuse>
          <specular>0.1 0.1 0.1 1.0</specular>
        </material>
      </visual>

      <collision name="camera_collision">
        <geometry>
          <box>
            <size>0.01 0.01 0.01</size> 
          </box>
        </geometry>
      </collision>

      <sensor name="camera" type="camera">
        <!-- Align sensor pose with the link without additional rotation -->
        <pose relative_to='camera_link'>0 0 0 0 0 0</pose>
        <camera>
          <horizontal_fov>1.047</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
          </image>
          <clip>
            <near>0.05</near>
            <far>200</far>
          </clip>
        </camera>
        <!-- Set frame ID appropriately, typically 'camera_link' -->
        <gz_frame_id>camera_link</gz_frame_id>

        <always_on>1</always_on>
        <update_rate>10</update_rate>
        <visualize>1</visualize>
      </sensor>
    </link>

    <joint name="camera_joint" type="fixed">
      <parent>base_link</parent>
      <child>camera_link</child>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <joint name="FrontRightwheelSteeringJoint" type="revolute">
      <parent>base_link</parent>
      <child>FrontRightwheelSteering</child>
      <pose relative_to="base_link">.112 -.10 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.6</lower>
          <upper>0.6</upper>
          <velocity>1000</velocity>
          <effort>1000</effort>
        </limit>
      </axis>
    </joint>

    <joint name="FrontRightwheelJoint" type="revolute">
      <parent>FrontRightwheelSteering</parent>
      <child>FrontRightwheel</child>
      <pose relative_to="FrontRightwheelSteeringJoint">0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>
    <joint name="FrontLeftwheelSteeringJoint" type="revolute">
      <parent>base_link</parent>
      <child>FrontLeftwheelSteering</child>
      <pose relative_to="base_link">.112 .10 0 0 0 0</pose>
